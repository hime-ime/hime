#!/bin/sh
# hime filter jp2cn version 0.0.3
  read -r aa
  MATCH=`tail -n 681 $0 | grep $aa | head -n 1`
  if [ "$MATCH" = "" ]; then
    echo -n $aa
  else
    echo -n $aa | sed -e "$MATCH"
  fi
  exit
