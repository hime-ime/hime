#!/bin/bash
# hime filter nobopomofo version 0.0.4
  read -r -t 1 aa
  echo -n $aa
  if [ "$(echo -n $aa | grep -v -f $0 --)" = "" ]; then
    declare -i bb=$(date +%N | tr -d 0)%2
    if [ "$bb" = "1" ]; then
      hime-message -icon /usr/share/pixmaps/hime/SS1135_ST.jpg -duration 1500 &
    else
      hime-message -icon /usr/share/pixmaps/hime/SS1208_DT.jpg -duration 1500 &
    fi
  fi
  exit
